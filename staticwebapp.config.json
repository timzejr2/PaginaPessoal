{
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["*.{css,scss,js,png,gif,ico,jpg,svg}"]
    },
    "routes": [
        {
            "route": "/blog",
            "redirect": "/blog",
            "statusCode": 301
        },
        {
            "route": "/projects",
            "redirect": "/projects",
            "statusCode": 301
        },
        {
            "route": "/blog/post/*",
            "redirect": "/blog/post/*",
            "statusCode": 301
        },
        {
            "route": "/projects/post/*",
            "redirect": "/projects/post/*",
            "statusCode": 301
        }
    ]
  }